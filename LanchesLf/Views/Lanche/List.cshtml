@using LanchesLf.Models;
@model IEnumerable<Lanche>

<div><h2>Todos os Lanches</h2></div>

@foreach (var Lanche in Model)
{
    <div class="col-sm-4 col-lg-4 col-md-4">
        <div class="img-thumbnail">
            <img src="@Lanche.ImagemUrl" class="img-rounded" alt="" />
        </div>
        <div class="caption">
            <h3 class="pull-righ"> @Lanche.Preco.ToString()</h3>
            <h3>
                @Lanche.Nome
            </h3>
            <p>@Lanche.DescricaoCurta</p>
        </div>
    </div>
}

